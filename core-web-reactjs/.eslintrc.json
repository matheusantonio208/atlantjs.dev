{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier/react"

    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/static-property-placement":"off",
        "react/jsx-filename-extension": ["warn", {"extensions": [".jsx", ".js"] } ],
        "react/state-in-constructor":"off"
    }
}
