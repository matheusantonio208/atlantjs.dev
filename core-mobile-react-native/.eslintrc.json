{
    "extends": [
        "plugin:react/recommended",
        "prettier/react",
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "__DEV__":"readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/static-property-placement":"off",
        "react/jsx-filename-extension": ["warn", {"extensions": [".jsx", ".js"] } ],
        "react/state-in-constructor":"off",
        "no-console": ["error",{"allow": ["tron"]}]
    }
}