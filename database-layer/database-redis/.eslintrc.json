{
    "rules": {
        "import-helpers/order-imports": [
          "warn",
          {
            "newlinesBetween": "always",
            "groups": [
              "/^#jobs/"
            ]
          }
        ]
    },
    "settings": {
        "import/resolver": {
            "babel-plugin-root-import": [
                {
                    "rootPathSuffix": "./src/jobs",
                    "rootPathPrefix": "#jobs/"
                }
            ]
        }
    }
}
