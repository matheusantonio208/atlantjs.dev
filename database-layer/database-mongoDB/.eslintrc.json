{
    "rules": {
        "import-helpers/order-imports": [
            "warn",
            {
                "newlinesBetween": "always",
                "groups": [
                    "/^#schemas/"
                ]
            }
        ]
    },
    "settings": {
        "import/resolver": {
            "babel-plugin-root-import": [
                {
                    "rootPathSuffix": "./src/schemas",
                    "rootPathPrefix": "#schemas/"
                }
            ]
        }
    }
}